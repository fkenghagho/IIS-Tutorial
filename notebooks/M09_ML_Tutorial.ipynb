   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from sklearn.neural_network import MLPClassifier\n",
    "\n",
    "# 1. DATA GENERATOR (Experience E)\n",
    "def generate_color_data(n_samples=1000):\n",
    "    # Centroids in RGB space\n",
    "    centroids = {\n",
    "        0: [150, 0, 150],   # Purple\n",
    "        1: [0, 200, 200],   # Cyan\n",
    "        2: [255, 100, 180], # Pink\n",
    "        3: [255, 140, 0],   # Orange\n",
    "        4: [20, 20, 20]     # Black (Background)\n",
    "    }\n",
    "    \n",
    "    X, y = [], []\n",
    "    for label, color in centroids.items():\n",
    "        # Add noise to simulate sensor variance\n",
    "        samples = np.array(color) + np.random.normal(0, 65, (n_samples, 3))\n",
    "        X.append(np.clip(samples, 0, 255))\n",
    "        y.extend([label] * n_samples)\n",
    "        \n",
    "    return np.vstack(X) / 255.0, np.array(y) # Normalize to [0, 1]\n",
    "\n",
    "# 2. THE LEARNER (Optimizing Theta)\n",
    "X_train, y_train = generate_color_data()\n",
    "\n",
    "# We use MLP with log-loss (Cross Entropy)\n",
    "# Note: MLPClassifier in sklearn uses Softmax internally for multi-class\n",
    "clf = MLPClassifier(hidden_layer_sizes=(16, 16), activation='relu', \n",
    "                    solver='adam', max_iter=500, random_state=1)\n",
    "clf.fit(X_train, y_train)\n",
    "\n",
    "# 3. THE INFERENCE (Resolving State X)\n",
    "def run_inference(model, image_array):\n",
    "    h, w, c = image_array.shape\n",
    "    pixels = image_array.reshape(-1, 3) / 255.0\n",
    "    \n",
    "    # Get Softmax probabilities\n",
    "    probs = model.predict_proba(pixels)\n",
    "    \n",
    "    # The classification result\n",
    "    preds = np.argmax(probs, axis=1)\n",
    "    \n",
    "    # Map back to colors for visualization\n",
    "    label_to_color = {\n",
    "        0: [128, 0, 128], # Purple\n",
    "        1: [0, 255, 255], # Cyan\n",
    "        2: [255, 192, 203],# Pink\n",
    "        3: [255, 165, 0],  # Orange\n",
    "        4: [0, 0, 0]       # Black\n",
    "    }\n",
    "    \n",
    "    output_img = np.array([label_to_color[p] for p in preds])\n",
    "    return output_img.reshape(h, w, 3).astype(np.uint8), clf.loss_curve_\n",
    "\n",
    "\n",
    "# --- VISUALIZATION ---\n",
    "\n",
    "# Create a fake \"test image\" with blocks of color and noise\n",
    "test_img = np.zeros((100, 100, 3))+30\n",
    "test_img[10:40, 10:40] = [140, 10, 140]  # Noisy Purple\n",
    "test_img[50:80, 60:90] = [240, 150, 10] # Noisy Orange\n",
    "test_img += np.random.normal(0, 20, test_img.shape)\n",
    "test_img = np.clip(test_img, 0, 255).astype(np.uint8)\n",
    "original_img=test_img.copy()\n",
    "\n",
    "segmented_img, loss_history = run_inference(clf, test_img)\n",
    "\n",
    "# Display everything\n",
    "plt.figure(figsize=(15, 5))\n",
    "\n",
    "# Plot 1: Learning Curve (Cross Entropy Loss)\n",
    "plt.subplot(1, 3, 1)\n",
    "plt.plot(loss_history, color='red', lw=2)\n",
    "plt.title(\"Learning: Cross Entropy Loss\")\n",
    "plt.xlabel(\"Iteration (Epoch)\"); plt.ylabel(\"Loss\")\n",
    "\n",
    "# Plot 2: Original \"Sensor\" Input\n",
    "plt.subplot(1, 3, 2)\n",
    "plt.imshow(test_img)\n",
    "plt.title(\"Input Z (Noisy Pixels)\")\n",
    "plt.axis('off')\n",
    "\n",
    "# Plot 3: Inference Result (Categorized State)\n",
    "plt.subplot(1, 3, 3)\n",
    "plt.imshow(segmented_img)\n",
    "plt.title(\"Inference X (Categorized)\")\n",
    "plt.axis('off')\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
