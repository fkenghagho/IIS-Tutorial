   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from sklearn.cluster import KMeans\n",
    "\n",
    "# 1. Scene-Wide Color Clustering\n",
    "def segment_scene_by_color(original_img, n_clusters=3):\n",
    "    \"\"\"\n",
    "    Clusters the entire image into K colors without filtering for a specific label.\n",
    "    \"\"\"\n",
    "    h, w, _ = original_img.shape\n",
    "    \n",
    "    # Flatten and normalize ALL pixels in the image\n",
    "    all_pixels_rgb = original_img.reshape(-1, 3) / 255.0\n",
    "    \n",
    "    # K-Means looks at every pixel to find the 'K' most dominant color groups\n",
    "    kmeans = KMeans(n_clusters=n_clusters, random_state=1, n_init=10)\n",
    "    cluster_ids = kmeans.fit_predict(all_pixels_rgb)\n",
    "\n",
    "    # Reshape back to the image grid\n",
    "    instance_map = (cluster_ids + 1).reshape(h, w)\n",
    "    \n",
    "    # Calculate the average color of each cluster for plotting\n",
    "    cluster_colors = []\n",
    "    for i in range(n_clusters):\n",
    "        avg_color = np.mean(all_pixels_rgb[cluster_ids == i], axis=0) * 255\n",
    "        cluster_colors.append(avg_color.astype(np.uint8))\n",
    "        \n",
    "    return instance_map, cluster_colors\n",
    "\n",
    "# 2. Debugging Multiple Global Clusters\n",
    "def visualize_scene(original_img, mlp_labels, instance_map, cluster_colors):\n",
    "    # Mapping for MLP classification colors (Panel 2)\n",
    "    label_to_color = {0: [128,0,128], 1: [0,255,255], 2: [255,192,203], 3: [255,165,0], 4: [0,0,0]}\n",
    "    \n",
    "    unique_clusters = np.unique(instance_map)\n",
    "    k = len(unique_clusters)\n",
    "\n",
    "    total_plots = 2 + k\n",
    "    fig, ax = plt.subplots(1, total_plots, figsize=(4 * total_plots, 4))\n",
    "\n",
    "    # Panel 0: Raw Sensor Data\n",
    "    ax[0].imshow(original_img)\n",
    "    ax[0].set_title(\"1. Original Image\")\n",
    "    ax[0].axis('off')\n",
    "\n",
    "    # Panel 1: MLP Classification (The 'Cortex' reference)\n",
    "    mlp_debug_view = np.zeros_like(original_img)\n",
    "    for label_id, col in label_to_color.items():\n",
    "        mlp_debug_view[mlp_labels == label_id] = col\n",
    "    ax[1].imshow(mlp_debug_view)\n",
    "    ax[1].set_title(\"2. MLP Classification\")\n",
    "    ax[1].axis('off')\n",
    "\n",
    "    # Panels 2 to End: The individual Color Masks found by K-Means\n",
    "    for i, cluster_id in enumerate(unique_clusters):\n",
    "        mask_display = np.zeros_like(original_img)\n",
    "        # Use the actual average color found by K-Means for this mask\n",
    "        mask_display[instance_map == cluster_id] = cluster_colors[i]\n",
    "        \n",
    "        ax[i+2].imshow(mask_display)\n",
    "        ax[i+2].set_title(f\"Cluster {cluster_id} Mask\")\n",
    "        ax[i+2].axis('off')\n",
    "\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "# --- EXECUTION ---\n",
    "\n",
    "# 1. Run Inference (MLP)\n",
    "pixels_norm = original_img.reshape(-1, 3) / 255.0\n",
    "labels_mlp = clf.predict(pixels_norm).reshape(original_img.shape[0], original_img.shape[1])\n",
    "\n",
    "# 2. Run Global Clustering (K-Means)\n",
    "# We ask for 3 clusters to find Black, Purple, and Orange\n",
    "k_val = 3\n",
    "instance_mask, colors_found = segment_scene_by_color(original_img, n_clusters=k_val)\n",
    "\n",
    "# 3. Final Plot\n",
    "visualize_scene(original_img, labels_mlp, instance_mask, colors_found)"
   ]
