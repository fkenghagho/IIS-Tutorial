    "import numpy as np\n",
    "import random\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# 1. SETUP THE ENVIRONMENT\n",
    "# 10 distances (0=very close, 9=very far) | 2 actions (0: High, 1: Low)\n",
    "Q_table = np.zeros((10, 2)) \n",
    "epsilon = 0.2    # 20% Exploration (chance to try something random)\n",
    "alpha = 0.1      # Learning rate (how fast it updates beliefs)\n",
    "gamma = 0.9      # Discount factor (importance of future potential)\n",
    "episodes = 500   # Number of training attempts\n",
    "\n",
    "# 2. DEFINE THE BRAIN FUNCTIONS\n",
    "def choose_action(state):\n",
    "    \"\"\"Epsilon-Greedy Policy\"\"\"\n",
    "    if random.random() < epsilon:\n",
    "        return random.randint(0, 1) # Explore: Try random approach\n",
    "    else:\n",
    "        return np.argmax(Q_table[state]) # Exploit: Use best known belief\n",
    "\n",
    "def update_q(s, a, reward, s_next):\n",
    "    \"\"\"The Bellman Equation (TD Learning)\"\"\"\n",
    "    # Reality = Current Reward + Potential of next state\n",
    "    td_target = reward + gamma * np.max(Q_table[s_next])\n",
    "    \n",
    "    # Update rule: Belief = Belief + (Alpha * Surprise)\n",
    "    Q_table[s, a] += alpha * (td_target - Q_table[s, a])\n",
    "\n",
    "# 3. THE EXECUTION (Training Loop)\n",
    "for i in range(episodes):\n",
    "    state = random.randint(0, 9) # Start at a random distance\n",
    "    \n",
    "    # Decision: How should I approach?\n",
    "    action = choose_action(state)\n",
    "    \n",
    "    # Environment Physics (The Ground Truth)\n",
    "    # Rule: If Far (>=5) High is better. If Close (<5) Low is better.\n",
    "    if (state >= 5 and action == 0) or (state < 5 and action == 1):\n",
    "        reward = 10  # Success!\n",
    "    else:\n",
    "        reward = -5  # Failure/Collision\n",
    "    \n",
    "    # Next State: Simulation of moving slightly after attempt\n",
    "    next_state = max(0, state - 1)\n",
    "    \n",
    "    # Update the Q-matrix with the result\n",
    "    update_q(state, action, reward, next_state)\n",
    "\n",
    "# 4. VISUALIZATION\n",
    "print(\"Final Q-Table (Learned Beliefs):\")\n",
    "print(Q_table)\n",
    "\n",
    "plt.figure(figsize=(8, 6))\n",
    "plt.imshow(Q_table, cmap='viridis', aspect='auto')\n",
    "plt.colorbar(label='Action Value (Q)')\n",
    "plt.xticks(ticks=[0, 1], labels=['High Approach', 'Low Approach'])\n",
    "plt.ylabel('Distance (0:Close, 9:Far)')\n",
    "plt.title('Exercise 3: The Learned Decision Map')\n",
    "plt.show()"
   ]
