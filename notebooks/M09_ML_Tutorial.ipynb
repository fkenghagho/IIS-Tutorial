    "import numpy as np\n",
    "import random\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# 1. SETUP THE ENVIRONMENT\n",
    "# 10 distances (0=very close, 9=very far) | 2 actions (0: High (+3), 1: Low (+1))\n",
    "Q_table = np.zeros((10, 2)) \n",
    "epsilon = 0.10   # 20% Exploration\n",
    "alpha = 0.1      # Learning rate\n",
    "gamma = 0.9      # Discount factor\n",
    "episodes = 10000  # Increased episodes to allow the \"Braking\" logic to converge\n",
    "\n",
    "# 2. DEFINE THE BRAIN FUNCTIONS\n",
    "def choose_action(state):\n",
    "    \"\"\"Epsilon-Greedy Policy\"\"\"\n",
    "    if random.random() < epsilon:\n",
    "        return random.randint(0, 1) \n",
    "    else:\n",
    "        return np.argmax(Q_table[state]) \n",
    "\n",
    "def update_q(s, a, reward, s_next):\n",
    "    \"\"\"The Bellman Equation\"\"\"\n",
    "    td_target = reward + gamma * np.max(Q_table[s_next])\n",
    "    Q_table[s, a] += alpha * (td_target - Q_table[s, a])\n",
    "\n",
    "# 3. THE EXECUTION (Training Loop)\n",
    "for i in range(episodes):\n",
    "    # Start at a random distance between 1 and 9 (0 is the goal)\n",
    "    state = random.randint(1, 9) \n",
    "    \n",
    "    # Observe state and choose action\n",
    "    action = choose_action(state)\n",
    "    \n",
    "    #  PHYSICS OF THE ACTION \n",
    "    if action == 0: # High Approach\n",
    "        step = 3\n",
    "    else:           # Low Approach\n",
    "        step = 1\n",
    "        \n",
    "    # Calculate Next State before reward\n",
    "    next_state = max(0, state - step)\n",
    "    \n",
    "    #  REWARD LOGIC (The Feedback) \n",
    "    if action == 0 and state < 3:\n",
    "        # CRASH: High speed approach when too close to target\n",
    "        reward = -50 \n",
    "    elif next_state == 0 and action == 1:\n",
    "        # SUCCESS: Gentle docking at the goal\n",
    "        reward = 20\n",
    "    elif action == 0 and state >= 3:\n",
    "        # GOOD: Efficiently covering ground while far away\n",
    "        reward = 5\n",
    "    else:\n",
    "        # NEUTRAL: Moving forward safely but slowly\n",
    "        reward = 1\n",
    "\n",
    "    # Update the Q-matrix with the observed transition\n",
    "    update_q(state, action, reward, next_state)\n",
    "\n",
    "# 4. VISUALIZATION\n",
    "print(\"Final Q-Table (High Approach | Low Approach):\")\n",
    "print(np.round(Q_table, 2))\n",
    "\n",
    "plt.figure(figsize=(10, 7))\n",
    "plt.imshow(Q_table, cmap='RdYlGn', aspect='auto') # Red-Yellow-Green scale\n",
    "plt.colorbar(label='Action Value (Expected Reward)')\n",
    "plt.xticks(ticks=[0, 1], labels=['High Approach (+3)', 'Low Approach (+1)'])\n",
    "plt.yticks(ticks=np.arange(10))\n",
    "plt.ylabel('Current Distance (0: At Goal, 9: Far)')\n",
