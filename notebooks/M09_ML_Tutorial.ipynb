    "import time\n",
    "from pyswip import Prolog\n",
    "\n",
    "prolog = Prolog()\n",
    "\n",
    "# 1. THE DOMAIN THEORY (Deep Kinematic Chain)\n",
    "# Creating a 50-link chain to ensure the recursive search is measurable\n",
    "num_links = 50\n",
    "nodes = [f\"link_{i}\" for i in range(num_links)]\n",
    "\n",
    "for i in range(len(nodes)-1):\n",
    "    prolog.assertz(f\"connected({nodes[i]}, {nodes[i+1]})\")\n",
    "\n",
    "# The Transitivity Rules\n",
    "prolog.assertz(\"linked(X, Y) :- connected(X, Y)\")\n",
    "prolog.assertz(\"linked(X, Y) :- connected(X, Z), linked(Z, Y)\")\n",
    "\n",
    "# 2. FIRST QUERY: PURE DEDUCTION (The \"Slow\" Path)\n",
    "# The robot must traverse the depth of the chain recursively\n",
    "start_time = time.time()\n",
    "result1 = list(prolog.query(f\"linked({nodes[0]}, {nodes[-1]})\"))\n",
    "end_time = time.time()\n",
    "t1 = end_time - start_time\n",
    "print(f\"First Inference (Recursive): {t1:.6f} seconds\")\n",
    "\n",
    "# 3. THE DEDUCTIVE LEARNING STEP\n",
    "# We 'compile' the logic by asserting the conclusion at the TOP of the KB\n",
    "if result1:\n",
    "    prolog.asserta(f\"linked({nodes[0]}, {nodes[-1]})\")\n",
    "    print(f\"Logic Compiled: Shortcut created from {nodes[0]} to {nodes[-1]}\")\n",
    "\n",
    "# 4. SECOND QUERY: RETRIEVAL (The \"Fast\" Path)\n",
    "# Prolog finds the shortcut immediately via unification\n",
    "start_time = time.time()\n",
    "result2 = list(prolog.query(f\"linked({nodes[0]}, {nodes[-1]})\"))\n",
    "end_time = time.time()\n",
    "t2 = end_time - start_time\n",
    "\n",
    "print(f\"Second Inference (Learned):   {t2:.6f} seconds\")\n",
    "print(f\"--- Speedup: {t1/t2:.2f}x faster ---\")"
   ]
