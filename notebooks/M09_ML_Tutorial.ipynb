    "import time\n",
    "from pyswip import Prolog\n",
    "\n",
    "prolog = Prolog()\n",
    "\n",
    "# 1. THE DOMAIN THEORY (The \"Logic\" of the Robot Chain)\n",
    "# Creating a 2000-link chain to force measurable work\n",
    "num_links = 20000 \n",
    "nodes = [f\"link_{i}\" for i in range(num_links)]\n",
    "for i in range(len(nodes)-1):\n",
    "    prolog.assertz(f\"connected({nodes[i]}, {nodes[i+1]})\")\n",
    "\n",
    "# Transitivity Rules: Right-Recursive (Stable)\n",
    "prolog.assertz(\"linked(X, Y) :- connected(X, Y)\")\n",
    "prolog.assertz(\"linked(X, Y) :- connected(X, Z), linked(Z, Y)\")\n",
    "\n",
    "# 2. FIRST QUERY: DEDUCTION (The \"Naive\" Agent)\n",
    "start_time = time.time()\n",
    "query_str = f\"linked({nodes[0]}, {nodes[-1]})\"\n",
    "result = list(prolog.query(query_str))\n",
    "t1 = time.time() - start_time\n",
    "\n",
    "print(f\"First Inference (Recursive): {t1:.6f} seconds\")\n",
    "\n",
    "# 3. DEDUCTIVE LEARNING: Lemma Incorporation\n",
    "if result:\n",
    "    # Use ASSERTA to place the 'learned' fact at the top of the search tree\n",
    "    prolog.asserta(f\"linked({nodes[0]}, {nodes[-1]})\")\n",
    "    print(f\"Learning: Shortcut '{nodes[0]} -> {nodes[-1]}' synthesized.\")\n",
    "\n",
    "# 4. SECOND QUERY: RETRIEVAL (The \"Educated\" Agent)\n",
    "start_time = time.time()\n",
    "list(prolog.query(query_str))\n",
    "t2 = time.time() - start_time\n",
    "\n",
    "print(f\"Second Inference (Learned):   {t2:.6f} seconds\")\n",
    "print(f\"--- Speedup Factor: {t1/t2:.2f}x ---\")"
   ]
